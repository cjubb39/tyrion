TB_LOG2 ?= 8
CLK_CYCLE_PS ?= 960
DRAMSIM_CPU_CLK_SPEED ?= 1000000000
CLOCK_RATIO_SYSC_WAIT ?= 5
CLOCK_RATIO_DRAMSIM_UPDATE ?= 4

CFLAGS += -DTB_LOG2=$(TB_LOG2) -DCLK_CYCLE_PS=$(CLK_CYCLE_PS) \
					-DDRAMSIM_CPU_CLK_SPEED=$(DRAMSIM_CPU_CLK_SPEED) \
					-DCLOCK_RATIO_SYSC_WAIT=$(CLOCK_RATIO_SYSC_WAIT) \
					-DCLOCK_RATIO_DRAMSIM_UPDATE=$(CLOCK_RATIO_DRAMSIM_UPDATE)

